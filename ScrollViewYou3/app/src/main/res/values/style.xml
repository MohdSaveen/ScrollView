<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textview1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginTop">70dp</item>
        <item name="android:layout_marginStart">30dp</item>
    </style>
    <style name="view1">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginStart">25dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginEnd">25dp</item>
        <item name="android:background">@color/black</item>
    </style>
    <style name="textview2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
    </style>
    <style name="view2">
        <item name="android:layout_width">350dp</item>
        <item name="android:layout_height">53dp</item>
        <item name="android:layout_centerHorizontal">true</item>
        <item name="android:layout_marginStart">100dp</item>
    </style>
    <style name="imageViews">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">65dp</item>
    </style>
    <style name="textViews3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_marginStart">120dp</item>
    </style>
